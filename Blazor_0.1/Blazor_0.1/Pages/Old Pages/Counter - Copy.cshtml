@page "/mypage"
@inject Blazor_0._1.Localization.Localizator localizator
<h1>AZAZAZAZA</h1>

<p>Current count: @currentCount @localizator.Current.Headers.SignUpTitle</p>

<button class="btn btn-primary" onclick="@IncrementCount">Click me</button>

@functions {
    int currentCount = 0;

    void IncrementCount()
    {
        currentCount++;
    }
}
